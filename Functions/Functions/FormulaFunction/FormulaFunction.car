		<FormulaFunctionDef name="min" type="FormulaFunctionDef" id="fba26955-4659-47b8-8142-5e7073f796e0">
			<description>min of value</description>
			<Category>User Defined</Category>
			<parameterfields>
				<parameterfield>
					<name>v1</name>
					<type>Any</type>
				</parameterfield>
				<parameterfield>
					<name>v2</name>
					<type>Any</type>
				</parameterfield>
			</parameterfields>
			<ReturnType>Any</ReturnType>
			<Code><![CDATA[return v1 < v2 ? v1 : v2;]]></Code>
			<function-type>Formula</function-type>
		</FormulaFunctionDef>
		<FormulaFunctionDef name="secSumDouble1" type="FormulaFunctionDef" id="102b3b36-d0f0-4eb2-be30-9dfce44693fd">
			<description></description>
			<Category>User Defined</Category>
			<parameterfields>
				<parameterfield>
					<name>section</name>
					<type>Section</type>
				</parameterfield>
				<parameterfield>
					<name>fieldName</name>
					<type>String</type>
				</parameterfield>
			</parameterfields>
			<ReturnType>Double</ReturnType>
			<Code><![CDATA[def sum = 0.0;
foreach($elm in section) {
	if(!isNull($elm, fieldName)) {
		sum += getDouble($elm, fieldName);
	}
}

return sum;
]]></Code>
			<function-type>Formula</function-type>
		</FormulaFunctionDef>
		<FormulaFunctionDef name="secSumDouble2" type="FormulaFunctionDef" id="acbfc1c9-8bdc-4f25-955d-4ce5d73a428d">
			<description></description>
			<Category>User Defined</Category>
			<parameterfields>
				<parameterfield>
					<name>section</name>
					<type>Any</type>
				</parameterfield>
				<parameterfield>
					<name>fieldName</name>
					<type>Token</type>
				</parameterfield>
			</parameterfields>
			<ReturnType>Double</ReturnType>
			<Code><![CDATA[def sum = 0.0;
foreach($elm in section) {
	if(!isNull($elm.fieldName)) {
		sum += $elm.fieldName;
	}
}
return sum;
]]></Code>
			<function-type>Formula</function-type>
